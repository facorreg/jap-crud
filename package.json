{
  "name": "jap_app_back",
  "version": "1.0.0",
  "description": "Backend for my japanese api",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test; jest",
    "lint": "eslint --fix --ignore-path .gitignore \"./**/*.js\" \"./**/*.jsx\"",
    "dev": "NODE_ENV=developement webpack --watch --mode development",
    "build": "NODE_ENV=prod webpack --mode production",
    "start": "node ./dist/bundle.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,js}",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/coverage/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "moduleNameMapper": {
      "^@GET(.*)$": "<rootDir>/src/GET$1",
      "^@CREATE(.*)$": "<rootDir>/src/CREATE$1",
      "^@UPDATE(.*)$": "<rootDir>/src/UPDATE$1",
      "^@DELETE(.*)$": "<rootDir>/src/DELETE$1",
      "^@COMMON(.*)$": "<rootDir>/src/COMMON$1",
      "^@lodash(.*)$": "<rootDir>/src/utils/lodash/index.js",
      "^@mongo(.*)$": "<rootDir>/src/mongo$1",
      "^@models(.*)$": "<rootDir>/src/mongo/models$1",
      "^@utils(.*)$": "<rootDir>/src/utils/index.js",
      "^@test-utils(.*)$": "<rootDir>/__tests__/COMMON/utils/index.js",
      "^@TEST-COMMON(.*)$": "<rootDir>/__tests__/COMMON$1"
    },
    "testEnvironment": "node",
    "testRegex": "\\/__tests__\\/(.*\\/).*(\\.test).js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/",
      "/coverage",
      ".eslintrc.js",
      "babel.config.js",
      "webpack.config.js",
      "webpack.shared.js",
      "dist"
    ]
  },
  "author": "facorreg",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-filename-rules": "^1.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-no-secrets": "^0.6.8",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "jest": "^26.6.3",
    "next-unused": "0.0.3",
    "nodemon": "^2.0.6",
    "nodemon-webpack-plugin": "^4.3.2",
    "prettier": "2.2.1",
    "supertest": "^6.1.1",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "getenv": "^1.0.0",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.12",
    "wanakana": "^4.0.2"
  }
}
